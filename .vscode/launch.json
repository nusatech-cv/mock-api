  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Mock",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/mocks/index.js",
      "runtimeExecutable": "${userHome}/.nvm/versions/node/v20.4.0/bin/node"
    },
    {
      "name": "Launch Generate Mock",
      "type": "node",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "program": "${workspaceFolder}/scripts/generate-mock.js",
      "runtimeExecutable": "${userHome}/.nvm/versions/node/v20.4.0/bin/node"
    }
  ]
}